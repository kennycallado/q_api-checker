{
  "name": "q-api-checker",
  "version": "0.1.4",
  "scripts": {
    "dev": "nodemon",
    "build": "npx esbuild src/server.ts --bundle --platform=node --target=node18 --minify --outdir=dist/ && sed -i 's/class{/class User{/g' dist/server.js",
    "start": "node ./dist/server.js"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "NODE_ENV=development ./node_modules/.bin/ts-node ./src/server.ts"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.1",
    "esbuild": "^0.17.19",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
